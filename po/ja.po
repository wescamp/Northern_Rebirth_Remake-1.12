#: Northern_Rebirth_Remake/scenarios/06a_Old_Friend.cfg:166
#: Northern_Rebirth_Remake/scenarios/07b_Weight_of_Revenge.cfg:327
#: Northern_Rebirth_Remake/scenarios/10a_Stolen_Gold.cfg:468
#: Northern_Rebirth_Remake/scenarios/10a_Stolen_Gold.cfg:635
msgid "Resist till the end of turns"
msgstr ""

#. [objective]: condition=lose
#: Northern_Rebirth_Remake/scenarios/06a_Old_Friend.cfg:174
msgid "Enemy units surviving within dwarven walls"
msgstr ""

#. [print]
#: Northern_Rebirth_Remake/scenarios/06a_Old_Friend.cfg:214
msgid "Veteran numbers: $veterans_force"
msgstr ""

