msgstr ""

#. [scenario]: id=05b_Hero_is_Dead
#: Northern_Rebirth_Remake/scenarios/05b_Hero_is_Dead.cfg:85
msgid "Hhhhhhhuu"
msgstr ""

#. [scenario]: id=05b_Hero_is_Dead
#: Northern_Rebirth_Remake/scenarios/05b_Hero_is_Dead.cfg:86
